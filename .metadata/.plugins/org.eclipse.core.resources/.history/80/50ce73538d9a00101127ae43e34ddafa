#include "stm32f4xx.h"
#include <stdint.h>
#include <stdio.h>
#include "uart.h"
#include "adc.h"
#include "systick.h"


// standard LED enable
#define GPIOAEN			(1U<<0)
#define GPIOA_5			(1U<<5)
#define LED_PIN			GPIOA_5



int main(void) {

	uart2_tx_init();


	while(1) {
		sensor_value = adc_read();
		printf("Sensor value: %d \n\r", (int)sensor_value);
	}


}







